---
export interface Props {
	qname1: string;
	qname2: string;
	qname3:string;
	qvalue1: string;
	qvalue2: string;
	qvalue3: string;
  	choice1:string
	choice2:string
	choice3:string
}

const { qname, qvalue1, qvalue2, qvalue3, choice1, choice2, choice3 } = Astro.props;
---

<div class="question">
<input type="radio" name={qname1} value={qvalue1}> {choice1} <br>
<input type="radio" name={qname2} value={qvalue2}> {choice2} <br>
<input type="radio" name={qname3} value={qvalue3}> {choice3} <br>
</div>
